function FadeIn()
{
	//=======================
	// ■オブジェクト(ギミック)
	//=======================

	// リソース読み込み
	local id_transfer = ReadGimmick_("o_com_01");

	// ■転送装置(崩落都市行)
	local transfer01 = ArrangePointGimmick_("o_com_01", id_transfer, "g_transfer01");

	// ■転送装置(凍骨の氷原行)
	local transfer02 = ArrangePointGimmick_("o_com_01", id_transfer, "g_transfer02");


	// ※浮島はイベント側で配置しないこと

	EventEnd_();
}

