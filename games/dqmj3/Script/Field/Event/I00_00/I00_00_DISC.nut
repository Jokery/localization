	// ■移動装置（歓楽の霊道）
	local transfer01 = ArrangePointGimmick_("o_com_01", id_transfer, "g_transfer01");

	// ■移動装置（黒鉄の監獄塔）
	local transfer02 = ArrangePointGimmick_("o_com_01", id_transfer, "g_transfer03");


	// ※浮島はイベント側で配置しないこと

	EventEnd_();
}

